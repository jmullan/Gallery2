msgstr "Strana %d z %d"

msgid "next &raquo;"
msgstr "další &raquo;"

msgid "last &raquo;"
msgstr "poslední &raquo;"

msgid "Username"
msgstr "Uživatelské jméno"

msgid "Full Name"
msgstr "Jméno a příjmení"

msgid "Email"
