#, c-format
msgid "WARNING: Cannot resize mime type %s"
msgstr "VARNING: Kan inte &auml;dra storlek mime-typ %s"

#, c-format
msgid "WARNING: Cannot compress mime type %s"
msgstr "VARNING: Kan inte komprimera mime-typ %s"

msgid "Resize complete"
msgstr ""

msgid "Resize Status"
msgstr "Status p&aring; F&ouml;r&auml;ndring av Storlek"

msgid "Resizing photo"
msgstr "&Auml;ndrar storlek p&aring; foto"

msgid "Size Limit"
msgstr "Storleksgr&auml;ns"

msgid "Define picture size limit"
msgstr "Definiera storleksgr&auml;n f&ouml;r bilder"

