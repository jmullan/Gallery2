#, c-format
msgid "WARNING: Cannot resize mime type %s"
msgstr "ADVARSEL: Kan ikke endre skalere MIME-type %s"

#, c-format
msgid "WARNING: Cannot compress mime type %s"
msgstr "ADVARSEL: Kan ikke komprimere MIME-type %s"

msgid "Resize complete"
msgstr "Skalering fullf&oslash;rt"

msgid "Resize Status"
msgstr "Skaleringsstatus"

msgid "Resizing photo"
msgstr "Skalerer foto"

msgid "Size Limit"
msgstr "Bildest&oslash;rrelse"

msgid "Define picture size limit"
msgstr "Angi grenseverdi for bildest&oslash;rrelse"

msgid "Display"
msgstr "Skjermvisning"

msgid "Maximum dimensions of full sized images"
msgstr "St&oslash;rste dimensjoner for bilder i full st&oslash;rrelse"

