#~ msgstr "Ezarpenak behar bezala gorde dira"

#~ msgid "Limit the slideshow to: "
#~ msgstr "Aurkezpena Mugatu: "

#~ msgid "The limit must be a positive number"
#~ msgstr "Muga zenbaki positibo bat izan behar da"

#~ msgid "Save"
#~ msgstr "Gorde"

#~ msgid "Reset"
#~ msgstr "Berrezarri"
