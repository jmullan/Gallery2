<?php

class LoadAlbumItemTestCase extends TestCase {

    var $_id = 1;

    function getIterations() {
	return array(500);
    }

    function getDescription() {
	return "Load AlbumItem with id $this->_id";
    }

    function testStart($iterations) {
	global $gallery;

	for ($i = 0; $i < $iterations; $i++) {
	    list ($ret, $entity) = $gallery->loadEntitiesById($this->_id);
	    if ($ret->isError()) {
		return $ret->wrap(__FILE__, __LINE__);
	    }
	}

	return GalleryStatus::success();
    }
}

?>

