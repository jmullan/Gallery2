/**
 * @package GalleryCore
 * @subpackage TestCase
 */
class ConstructorTestCase extends TestCase {

    function getIterations() {
	return array(1000, 50000, 100000);
    }

    function getDescription() {
	return 'Create a new GalleryEntity';
    }

    function testStart($iterations) {
	global $gallery;
	
	for ($i = 0; $i < $iterations; $i++) {
	    list ($ret, $entity) = GalleryFactory::newInstance('GalleryEntity', 'GalleryEntity');
	    if ($ret->isError()) {
		return $ret->wrap(__FILE__, __LINE__);
	    }
	}
	return GalleryStatus::success();
    }
}
?>
